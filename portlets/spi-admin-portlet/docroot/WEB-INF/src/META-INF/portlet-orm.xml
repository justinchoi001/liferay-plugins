<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.portal.resiliency.spi.model.impl.SPIDefinitionModelImpl">
		<attributes>
			<id name="spiDefinitionId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="name" />
			<basic name="connectorAddress" />
			<basic name="connectorPort" />
			<basic name="description" />
			<basic name="jvmArguments" />
			<basic name="portletIds" />
			<basic name="servletContextNames" />
			<basic name="typeSettings" />
			<basic name="status" />
			<basic name="statusMessage" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.portal.resiliency.spi.model.impl.SPIDefinitionImpl" name="SPIDefinition">
		<table name="SPIDefinition" />
		<attributes>
			<transient name="maxRestartAttempts" />
			<transient name="notificationRecipients" />
			<transient name="restartAttempts" />
			<transient name="typeSettingsProperties" />
		</attributes>
	</entity>
</entity-mappings>